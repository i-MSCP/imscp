    <copy_config mode="0644">${ROOT_CONF}/init/imscp_daemon.conf</copy_config>
    <copy_config mode="0644">${ROOT_CONF}/init/imscp_mountall.conf</copy_config>
    <copy_config mode="0644">${ROOT_CONF}/init/imscp_panel.conf</copy_config>
    <copy_config mode="0644">${ROOT_CONF}/init/imscp_panel.override</copy_config>
    <copy_config if="-f '/etc/init/php5.6-fpm.conf'" mode="0644">${ROOT_CONF}/init/php5.6-fpm.override</copy_config>
    <copy_config if="-f '/etc/init/php7.0-fpm.conf'" mode="0644">${ROOT_CONF}/init/php7.0-fpm.override</copy_config>
    <copy_config if="-f '/etc/init/php7.1-fpm.conf'" mode="0644">${ROOT_CONF}/init/php7.1-fpm.override</copy_config>
