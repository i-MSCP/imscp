                            <visible>0</visible>
                            <title_class>email</title_class>
                        </mail_account_edit_link>
                        <mail_account_quota_link>
                            <label>Edit mail quota</label>
                            <uri>/client/mail_quota.php</uri>
                            <visible>0</visible>
                            <title_class>email</title_class>
                        </mail_account_quota_link>
                        <enable_autoresponder_link>
                            <label>Enable auto responder</label>
                            <uri>/client/mail_autoresponder_enable.php</uri>
                            <visible>0</visible>
                            <title_class>email</title_class>
                        </enable_autoresponder_link>
                        <edit_autoresponder_link>
                            <label>Edit auto responder</label>
                            <uri>/client/mail_autoresponder_edit.php</uri>
                            <visible>0</visible>
                            <title_class>email</title_class>
                        </edit_autoresponder_link>
                    </pages>
                </overview>
                <add_email_account>
                    <label>Add email account</label>
                    <uri>/client/mail_add.php</uri>
                    <title_class>email</title_class>
                    <privilege_callback name="customerHasFeature" param="mail"/>
                </add_email_account>
                <catchall>
                    <label>Catchall</label>
                    <uri>/client/mail_catchall.php</uri>
                    <title_class>email</title_class>
                    <privilege_callback name="customerHasFeature" param="mail"/>
                    <pages>
                        <add_catchall_link>
                            <label>Add catchall</label>
                            <uri>/client/mail_catchall_add.php</uri>
                            <visible>0</visible>
                            <title_class>email</title_class>
                        </add_catchall_link>
                    </pages>
                </catchall>
                <external_mail_servers>
                    <label>External mail servers</label>
                    <uri>/client/mail_external.php</uri>
                    <title_class>email</title_class>
                    <privilege_callback name="customerHasFeature" param="external_mail"/>
                    <pages>
                        <add_ext_mail_server_link>
                            <label>Add external mail server for {DOMAIN_UTF8}</label>
                            <uri>/client/mail_external_add.php</uri>
                            <visible>0</visible>
                            <title_class>email</title_class>
                        </add_ext_mail_server_link>
                        <edit_ext_mail_server_link>
                            <label>Edit external mail server for {DOMAIN_UTF8}</label>
                            <uri>/client/mail_external_edit.php</uri>
                            <visible>0</visible>
                            <title_class>email</title_class>
                        </edit_ext_mail_server_link>
                    </pages>
                </external_mail_servers>
                <webmail>
                    <label>Webmail</label>
                    <uri>{WEBMAIL_PATH}</uri>
                    <target>{WEBMAIL_TARGET}</target>
                    <privilege_callback name="customerHasFeature" param="mail"/>
                </webmail>
            </pages>
